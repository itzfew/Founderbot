{
  "version": 2,
  "builds": [
    {
      "src": "public/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/pages/api/$1"
    },
    {
      "src": "/api/webhook",
      "dest": "/src/api/webhook.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.js"
    }
  ]
}
